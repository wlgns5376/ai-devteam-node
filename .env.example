# AI DevTeam Configuration
GITHUB_TOKEN="your_github_personal_access_token_here"     # Required: GitHub access token for repository operations
GITHUB_OWNER="your_github_username_or_organization"       # Required: GitHub username or organization
GITHUB_REPO="your_repository_name"                        # Optional: Single repository name (deprecated, use GITHUB_REPOS)
GITHUB_REPOS="owner1/repo1,owner2/repo2"                  # Optional: Comma-separated list of repositories to process
GITHUB_REPO_FILTER_MODE="whitelist"                       # Optional: Filter mode - 'whitelist' (only listed repos) or 'blacklist' (exclude listed repos)
GITHUB_PROJECT_NUMBER="your_github_project_id"                # Required: GitHub project ID for task management

# Developer Tools Configuration
CLAUDE_CODE_PATH="claude"                            # Optional: Path to Claude Code executable (default: claude-code)
CLAUDE_CODE_TIMEOUT="300000"                              # Optional: Claude Code timeout in milliseconds (default: 5 minutes)
GEMINI_CLI_PATH="gemini"                                  # Optional: Path to Gemini CLI executable (default: gemini)
GEMINI_CLI_TIMEOUT="300000"                               # Optional: Gemini CLI timeout in milliseconds (default: 5 minutes)

# Worker Pool Configuration
MIN_WORKERS="1"                                           # Optional: Minimum number of workers (default: 1)
MAX_WORKERS="5"                                           # Optional: Maximum number of workers (default: 5)
WORKER_TIMEOUT="600000"                                   # Optional: Worker timeout in milliseconds (default: 10 minutes)

# Workspace Configuration
WORKSPACE_ROOT="./workspace"                             # Optional: Root directory for workspaces (default: ./workspaces)
CLONE_DEPTH="1"                                           # Optional: Git clone depth (default: 1)

# Application Configuration
NODE_ENV="development"                                     # Optional: Node environment (development/production)
LOG_LEVEL="info"                                          # Optional: Logging level (error/warn/info/debug)
LOG_FILE="./logs/ai-devteam.log"                          # Optional: Log file path
PORT="3000"                                               # Optional: Application port (default: 3000)
MONITORING_INTERVAL_MS="30000"                            # Optional: Monitoring interval in milliseconds (default: 15000 for dev, 30000 for prod)
ENABLE_CONSOLE_LOG="false"                                # Optional: Enable console logging in production (default: false for production, true for development)

# Pull Request Comment Filtering Configuration
ALLOWED_PR_BOTS="sonarcloud[bot],deepsource[bot],codeclimate[bot],coderabbit[bot],copilot[bot],sourcery-ai[bot]"  # Optional: Comma-separated list of bots allowed in PR feedback
EXCLUDE_PR_AUTHOR="true"                                  # Optional: Exclude PR author comments from feedback processing (default: true)

# Task Master AI MCP Configuration
ANTHROPIC_API_KEY="your_anthropic_api_key_here"          # Optional: For task-master-ai Claude models
PERPLEXITY_API_KEY="your_perplexity_api_key_here"        # Optional: For task-master-ai research features
OPENAI_API_KEY="your_openai_api_key_here"                # Optional: For task-master-ai GPT models
GOOGLE_API_KEY="your_google_api_key_here"                # Optional: For task-master-ai Gemini models
XAI_API_KEY="your_xai_api_key_here"                      # Optional: For task-master-ai Grok models
OPENROUTER_API_KEY="your_openrouter_api_key_here"        # Optional: For task-master-ai multiple models
MISTRAL_API_KEY="your_mistral_api_key_here"              # Optional: For task-master-ai Mistral models
AZURE_OPENAI_API_KEY="your_azure_openai_api_key_here"    # Optional: For task-master-ai Azure OpenAI
OLLAMA_API_KEY="your_ollama_api_key_here"                # Optional: For task-master-ai local models